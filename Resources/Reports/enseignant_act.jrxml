	<queryString>
		<![CDATA[SELECT ACT.MEC_KEY,i.prenom||' '||i.nom_usuel ENSEIGNANT,TA.LIB_COURT,SUM(NVL(V.NB_HEURE_REALISE,0))  NB_HEURE_REALISE,SUM(NVL(V.NB_HEURES_VOEUX,0)) NB_HEURES_VOEUX
